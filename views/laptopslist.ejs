<%- include('header') -%>
<div
class="rounded bg-light  "
style="
  height: 5rem;
  text-align: center;
  font-size: 3rem;
  font-weight: 500;
"
>
<h2 style="box-shadow: 5px 5px 5px rgba(0, 0, 0, 0.159); color: rgb(110, 110, 110); font-weight: lighter;" class= "my-3 pb-3">Laptops list</h2>
</div>
<div class="container">
  <div class="row">
    <div class="col">
      <ul class="list-group">
        <% let len =laps.length;
         for(var i=0;i<len;i++) { %>
         <li
          class="list-group-item text-center btn-light"
          style=" background-color: white" >
        <a href="<%= laps[i].link %>" style="text-decoration: none;"> <b><%=laps[i].name%></b></a>
        </li>
        <% } %>
      </ul>
    </div>
  </div>
</div>
<%- include('footer') -%>